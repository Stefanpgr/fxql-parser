generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model FxqlRecords {
  id String @id @default(uuid())
  SourceCurrency      String @db.VarChar(4)
  DestinationCurrency String @db.VarChar(4)
  SellPrice           Decimal @db.Decimal(15,4)
  BuyPrice            Decimal @db.Decimal(15,4)
  CapAmount           Decimal @db.Decimal(15,4)
}
